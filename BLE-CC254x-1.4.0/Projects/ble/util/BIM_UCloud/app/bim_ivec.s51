/* ------------------------------------------------------------------------------------------------
 *                                      Constants
 * ------------------------------------------------------------------------------------------------
 */

; Image-A/B INTVEC Table relocation addresses set by the linker files of each image.
IMGA  EQU  0x0830
IMGB  EQU  0x4030

; Image-A must initialize this IDATA location in its reset vector handler with zero or non-zero.
; Image-B must reserve & not use this location.
;ISEL  EQU  0x09

;SAVE  EQU  0x08  ; This IDATA location must be reserved by the linker files of both images.

/* ------------------------------------------------------------------------------------------------
 *                                       Macros
 * ------------------------------------------------------------------------------------------------
 */

IV03  EQU  IMGA + 0x03
IV0b  EQU  IMGA + 0x0b
IV13  EQU  IMGA + 0x13
IV1b  EQU  IMGA + 0x1b
IV23  EQU  IMGA + 0x23
IV2b  EQU  IMGA + 0x2b
IV33  EQU  IMGA + 0x33
IV3b  EQU  IMGA + 0x3b
IV43  EQU  IMGA + 0x43
IV4b  EQU  IMGA + 0x4b
IV53  EQU  IMGA + 0x53
IV5b  EQU  IMGA + 0x5b
IV63  EQU  IMGA + 0x63
IV6b  EQU  IMGA + 0x6b
IV73  EQU  IMGA + 0x73
IV7b  EQU  IMGA + 0x7b
IV83  EQU  IMGA + 0x83
IV8b  EQU  IMGA + 0x8b

/* ------------------------------------------------------------------------------------------------
 *                                    8051 INTVEC Table
 * ------------------------------------------------------------------------------------------------
 */

  COMMON  INTVEC:CODE:ROOT(0)


  org     0x0003      ; RF FIFO
  ljmp    IMGB + 0x03 ; Jump to the corresponding Image-B INTVEC handler.

  org     0x000b      ; ADC
  ljmp    IMGB + 0x0b ; Jump to the corresponding Image-B INTVEC handler.

  org     0x0013      ; USART0 RX
  ljmp    IMGB + 0x13 ; Jump to the corresponding Image-B INTVEC handler.

  org     0x001b      ; USART1 RX
  ljmp    IMGB + 0x1b ; Jump to the corresponding Image-B INTVEC handler.

  org     0x0023      ; AES
  ljmp    IMGB + 0x23 ; Jump to the corresponding Image-B INTVEC handler.

  org     0x002b      ; Sleep Timer
  ljmp    IMGB + 0x2b ; Jump to the corresponding Image-B INTVEC handler.

  org     0x0033      ; Port2
  ljmp    IMGB + 0x33 ; Jump to the corresponding Image-B INTVEC handler.

  org     0x003b      ; USART0 TX
  ljmp    IMGB + 0x3b ; Jump to the corresponding Image-B INTVEC handler.

  org     0x0043      ; DMA
  ljmp    IMGB + 0x43 ; Jump to the corresponding Image-B INTVEC handler.

  org     0x004b      ; Timer1
  ljmp    IMGB + 0x4b ; Jump to the corresponding Image-B INTVEC handler.

  org     0x0053      ; Timer2
  ljmp    IMGB + 0x53 ; Jump to the corresponding Image-B INTVEC handler.

  org     0x005b      ; Timer3
  ljmp    IMGB + 0x5b ; Jump to the corresponding Image-B INTVEC handler.

  org     0x0063      ; Timer4
  ljmp    IMGB + 0x63 ; Jump to the corresponding Image-B INTVEC handler.

  org     0x006b      ; Port0
  ljmp    IMGB + 0x6b ; Jump to the corresponding Image-B INTVEC handler.

  org     0x0073      ; USART1 TX
  ljmp    IMGB + 0x73 ; Jump to the corresponding Image-B INTVEC handler.

  org     0x007b      ; Port1
  ljmp    IMGB + 0x7b ; Jump to the corresponding Image-B INTVEC handler.

  org     0x0083      ; RF General
  ljmp    IMGB + 0x83 ; Jump to the corresponding Image-B INTVEC handler.

  org     0x008b      ; WatchDog
  ljmp    IMGB + 0x8b ; Jump to the corresponding Image-B INTVEC handler.


__relay_err
  nop     ; Debugging catch-all - should never get here.
  ljmp    __relay_err

  END

